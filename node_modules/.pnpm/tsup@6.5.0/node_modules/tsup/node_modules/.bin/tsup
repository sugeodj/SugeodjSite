#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules/tsup/dist/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules/tsup/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules/tsup/dist/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules/tsup/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/tsup@6.5.0/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../dist/cli-default.js" "$@"
fi
