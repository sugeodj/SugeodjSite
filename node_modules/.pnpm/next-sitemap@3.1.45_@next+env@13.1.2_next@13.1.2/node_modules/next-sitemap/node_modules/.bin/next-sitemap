#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules/next-sitemap/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules/next-sitemap/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules/next-sitemap/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules/next-sitemap/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/next-sitemap@3.1.45_@next+env@13.1.2_next@13.1.2/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/next-sitemap.mjs" "$@"
else
  exec node  "$basedir/../../bin/next-sitemap.mjs" "$@"
fi
