#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/bin/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules/resolve/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/resolve@2.0.0-next.4/node_modules:/home/sugeodj/Developer/github/enji.dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
